<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.stockboo.view.custom.StockBooTextView android:id="@+id/scriptName" android:lines="1" android:textSize="14sp" android:textColor="@android:color/holo_blue_dark" android:layout_marginTop="5dp" android:layout_marginLeft="5dp" android:text="Stock Name" android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.stockboo.view.custom.StockBooTextView android:id="@+id/scriptID" android:textSize="12sp" android:layout_marginTop="5dp" android:layout_marginLeft="5dp" android:text="Stock ID" android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>